<div class="container">
    <h1>mysql database connection and select query</h1>
    <h2>{{title}}</h2>

    <a href="/employeeinfo/create-form" class="btn btn-success">Insert employeeinfo</a>
    <br /><br />
    <table class="table">
        <tr>
            <th>Emp_Id</th>
            <th>F_Name</th>
            <th>M_Name</th>
            <th>L_Name</th>
            <th>Job_Type</th>
            <th>Action</th>
        
        </tr>
        {{#each employeeinfo}}
        <tr>
            <td>{{this.Emp_Id}}</td>
            <td>{{this.F_Name}}</td>
            <td>{{this.M_Name}}</td>
            <td>{{this.L_Name}}</td>
            <td>{{this.Job_Type}}</td>
            
            <td>
                <a href="/employeeinfo/edit-form/{{this.Emp_Id}}" class="btn btn-info btn-sm">Update</a>
                <a href="/employeeinfo/delete/{{this.Emp_Id}}" class="btn btn-danger btn-sm">Delete</a>
            </td>
        </tr>
        {{/each}}
    </table>
</div>
