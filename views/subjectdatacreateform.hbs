
<div class="container">
  <h1>{{title}}</h1>
  <hr />
  <div class="row">
    <div class="col-md-6">
       <form action="/subjectdata/create" method="post">
       <div class="form-group">
              <input type="number" name="Sub_code" class="form-control" placeholder="Subject code" />
          </div>
          <div class="form-group">
              <input type="text" name="Sub_name" class="form-control"  placeholder="Subject Name" />
          </div>
          <div class="form-group">
              <input type="text" name="Semester" class="form-control"  placeholder="Semester" />
          </div>
          <div class="form-group">
              <button class="btn btn-info">Save Subject Data</button>
          </div>
      </form>
    </div>
  </div>
</div>