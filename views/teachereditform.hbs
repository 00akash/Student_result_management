
<div class="container">
  <h1>{{title}}</h1>
  <hr />
  <div class="row">
    <div class="col-md-6">
       <form action="/teacher/edit/{{teacher.Emp_Id}}" method="post">
          <div class="form-group">
              <input type="number" name="Emp_Id" class="form-control" value={{teacher.Emp_Id}} placeholder="Emp_Id" />
          </div>
          <div class="form-group">
              <input type="text" name="TGrade" class="form-control" value={{teacher.TGrade}} placeholder="TGread" />
          </div>
          <div class="form-group">
              <button class="btn btn-info">Save Teacher</button>
          </div>
      </form>
    </div>
  </div>
</div>